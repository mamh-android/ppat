<?xml version="1.0"?>

<project name="WiFi_on" basedir="." default="run">
  	<include file="${user.dir}/config/command.xml"/>

    <target name="run">
        <PowerCtrlUSB flag="on"/>
        <sleep seconds="5"/>
        <AdbCmd cmd="adb install -r resource/apk/WifiTest.apk"/>
    </target>

</project>
