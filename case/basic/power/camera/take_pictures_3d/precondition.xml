<?xml version="1.0"?>

<project name="power_take_picture" basedir="." default="run">
  	<include file="${user.dir}/config/command.xml"/>

    <target name="run">
        <PowerCtrlUSB flag="on"/>
        <AdbCmd cmd="adb push resource/bin/${resolution}/topicture.${os_version} /system/bin/topicture"/>
        <AdbCmd cmd="adb shell chmod 777 /system/bin/topicture"/>
    </target>
</project>
