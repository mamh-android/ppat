<?xml version="1.0" encoding="utf-8"?>
<opt>
  <clk>
    <core_clk>
      <clk>312000</clk>
      <node>/sys/devices/system/cpu/cpu0/cpufreq/scaling_setspeed</node>
      <r_node>/sys/devices/system/cpu/cpu0/cpufreq/cpuinfo_cur_freq</r_node>
      <name>CORE_CLK</name>
    </core_clk>
    <ddr_clk>
      <clk>797000</clk>
      <node>/sys/class/devfreq/devfreq-ddr/device/ddr_freq</node>
      <name>DDR_CLK</name>
    </ddr_clk>
    <gc_fclk>
      <clk>156000</clk>
      <node>/sys/devices/platform/galcore/gpu/gpu0/gpufreq/userspace/customize_rate</node>
      <r_node>/sys/devices/platform/galcore/gpu/gpu0/gpufreq/scaling_cur_freq</r_node>
      <name>GPU_FCLK</name>
    </gc_fclk>
    <gc2d_fclk>
      <clk>78000</clk>
      <node>/sys/devices/platform/galcore/gpu/gpu1/gpufreq/userspace/customize_rate</node>
      <r_node>/sys/devices/platform/galcore/gpu/gpu1/gpufreq/scaling_cur_freq</r_node>
      <name>GPU2D_FCLK</name>
    </gc2d_fclk>
    <vpu_fclk>
      <clk>156000</clk>
      <node>/sys/class/devfreq/devfreq-vpu.0/userspace/set_freq</node>
      <r_node>/sys/class/devfreq/devfreq-vpu.0/cur_freq</r_node>
      <name>VPU_FCLK</name>
    </vpu_fclk>
  </clk>
  <plug_in>
    <enable>0</enable>
    <type>1</type>
    <app_name>fc_filter.perl</app_name>
    <smart_level>2</smart_level>
    <vmin_delta>37500</vmin_delta>
    <!-- set vmin delta is one step -->
  </plug_in>
  <vmin>
    <vcc_main>
      <min>900000</min>
      <max>1300000</max>
      <step>12500</step>
      <order>0</order>
      <!-- 1: mean ascending,0:mean Descending -->
      <node>/sys/devices/platform/simple_dvfc/simple_dvfc/vbuck3</node>
      <name>vbuck3</name>
    </vcc_main>
  </vmin>
  <reboot_ctrl>
    <!-- method: relay or remote_ctrl -->
    <method>remote_ctrl</method>
    <relay>
      <serial_port>/dev/ttyS0</serial_port>
      <port>4</port>
      <onkey_port>3</onkey_port>
      <!--optinoal, for customer phone, press onkey to reboot system-->
    </relay>
    <remote_ctrl>
      <server_app>./rcc_utils</server_app>
      <client_app>./rcc_utilc</client_app>
      <client_cmd>vbus 0;powerOff;powerOn;onkey;vbus 1</client_cmd>
      <board_id>PXA1U88_DKB_V10_140</board_id>
    </remote_ctrl>
    <timeout>40</timeout>
    <force_reboot>0</force_reboot>
  </reboot_ctrl>
  <comm>
    <port>/dev/ttyUSB0</port>
    <board_no>DKB052_A0#114</board_no>
  </comm>
  <test_case>
    <!-- in this label, prepare command is set before case running -->
    <prepare>
		boot;
		setprop persist.sys.dump.enable 1;
		echo 1 &gt; /sys/devices/system/cpu/hotplug/lock;
		echo 1 &gt; /sys/devices/system/cpu/cpu1/online;
		echo 1 &gt; /sys/devices/system/cpu/cpu2/online;
		echo 1 &gt; /sys/devices/system/cpu/cpu3/online;
		echo userspace &gt; /sys/devices/platform/galcore/gpu/gpu0/gpufreq/scaling_cur_governor;
		#echo -n 114 &gt; /sys/class/android_usb/android0/iSerial; 
		#echo 0 &gt; /sys/class/android_usb/android0/enable; 
		#echo 1 &gt; /sys/class/android_usb/android0/enable;
		sleep 1;
		svc power stayon true;
		. /data/bin/p.sh;
		phs_cmd 5 manual;
		cg userspace;
		dg userspace;
		vg userspace;
		gcg userspace;
		gc2g userspace;
		echo userspace &gt; /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor;
		echo 0 &gt; /sys/class/devfreq/devfreq-ddr/polling_interval;
		echo 1 &gt; /sys/class/devfreq/devfreq-ddr/device/disable_ddr_fc;
		cd /data/worst_case;
		insmod simple_dvfc_mod.ko;
		input keyevent 82;
		sleep 1
	</prepare>
    <stop_3_fail>1</stop_3_fail>
    <cmd>export PATH=/data/bin:$PATH;export ATD_HW_PF=HELAN2;cd /data/worst_case;./wcc.sh 1</cmd>
    <timeout>90</timeout>
    <loop>1</loop>
    <result_file>/data/worst_case/wcc.xml</result_file>
    <workaround>0</workaround>
    <reg_dump>cd /data/worst_case;./reg_dump.sh</reg_dump>
    <reg_dump_file>/data/register.dump</reg_dump_file>
  </test_case>
  <power_monitor>
    <enable>0</enable>
    <start_app>ConsoleApplication1.exe</start_app>
    <timeout>5</timeout>
    <start_app_cmd>A</start_app_cmd>
    <read_app>ConsoleApplication3.exe</read_app>
    <read_app_cmd>B</read_app_cmd>
    <result_file>POWER.log</result_file>
  </power_monitor>
  <adb>
    <serial_no>PXA1U88_DKB_V10_140</serial_no>
    <old_serial_no>0123456789ABCDEF</old_serial_no>
    <enable>1</enable>
  </adb>
  <mail>
    <enable>0</enable>
    <from>wzsun@marvell.com</from>
    <to>wzsun@marvell.com;chenlzh@marvell.com;hhuan10@marvell.com</to>
  </mail>
  <perl>
    <!-- this label is to enable/disable perl transmition, if enabled, the following modules need to be install with "perl -MCPAN -e shell" and "install XML::Simple" as example,Data::Dumper,Spreadsheet::ParseExcel,Spreadsheet::ParseExcel::Workbook,Spreadsheet::WriteExcel,Tie::IxHash,Getopt::Long,XML::Simple,Spreadsheet::XLXS -->
    <enable>1</enable>
    <clk_select>CORE_CLK;DDR_CLK;GPU_FCLK;GPU2D_FCLK;VPU_FCLK</clk_select>
    <svc>
      <enable>0</enable>
      <dro>655</dro>
      <svc_tbl>EM_A0_SVC.xls</svc_tbl>
    </svc>
    <dll_phsel>
      <enable>0</enable>
      <x_axi>DLL_PHASE</x_axi>
      <y_axi>DLL_PHASE1</y_axi>
      <x_axi_range>0_31</x_axi_range>
      <y_axi_range>0_31</y_axi_range>
    </dll_phsel>
  </perl>
  <logcat>
    <!-- this label is to enable/disable logcat -->
    <enable>0</enable>
    <param>-v threadtime -r 50000 -n 5</param>
  </logcat>
  <xdb>
    <enable>0</enable>
    <!-- this flag is used to capture register when system issue happened -->
    <command>pxa988_launch.bat</command>
    <!-- xdb batch file name -->
    <path>C:\Marvell\XDB5.3\xdb</path>
    <!-- set xdb path -->
    <stop_run>0</stop_run>
    <!-- this flag is used to stop testing when system issue happened for xdb debug -->
  </xdb>
</opt>
