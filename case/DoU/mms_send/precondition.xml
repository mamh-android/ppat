<?xml version="1.0"?>

<project name="power_camcorder_record" basedir="." default="run">
  	<include file="${user.dir}/config/command.xml"/>

    <target name="run">
        <PowerCtrlUSB flag="on"/>
        <sleep seconds="5"/>
        <AdbCmd cmd="adb install resource/apk/MMSTest.apk"/>
        <AdbCmd cmd="adb push resource/test.jpg /${storage}/"/>
    </target>

</project>
