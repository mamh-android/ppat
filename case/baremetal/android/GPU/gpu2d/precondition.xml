<?xml version="1.0"?>

<project name="start_cpueater" basedir="." default="run"> 
    <include file="${user.dir}/config/command.xml"/>
    <target name="run" >
        <PowerCtrlUSB flag="on"/>
        <sleep seconds="5"/>
        <AdbCmd cmd="adb root"/>
        <AdbCmd cmd="adb remount"/>
        <AdbCmd cmd="adb push resource/gcuBenchMark /data/"/>
        <SerialPortCmd cmd="echo 0,1 > /sys/devices/platform/soc.2/d4200000.axi/mmp-gpu/gpu/control"/>
        <SerialPortCmd cmd="echo 0,1 > /sys/devices/platform/soc.2/d4200000.axi/mmp-gpu/gpu/debug/pm_state"/>
        <SerialPortCmd cmd="echo 1,1 > /sys/devices/platform/soc.2/d4200000.axi/mmp-gpu/gpu/debug/pm_state"/>
        <SerialPortCmd cmd="echo 0,0,0 > /sys/devices/platform/galcore/gpu/pwr_test"/>
        <SerialPortCmd cmd="echo 0,1,0 > /sys/devices/platform/galcore/gpu/pwr_test"/>
        <SerialPortCmd cmd="echo 0,2,0 > /sys/devices/platform/galcore/gpu/pwr_test"/>
        <SerialPortCmd cmd="echo 1,0,0 > /sys/devices/platform/galcore/gpu/pwr_test"/>
        <SerialPortCmd cmd="echo 1,1,0 > /sys/devices/platform/galcore/gpu/pwr_test"/>
        <SerialPortCmd cmd="echo 1,2,0 > /sys/devices/platform/galcore/gpu/pwr_test"/>
    </target>
</project>
