<!-- page-container -->
<div class="page-container tab-content"  >
    <!-- Tab #basic -->
    <div class="tab-pane active">
        <!-- Grid row-->
            <div class="row">
                <h2><span>Select Daily</span></h2>
                <%= form_tag("/compare/add_daily_compare", method: "post", remote: true) do %>
                    <ul>
                        <fieldset>
                        <li>
                            <select NAME="Platform" onChange="getBranch()">
                                <option VALUE="eden">eden </option>
                                <option VALUE="pxa1908">pxa1908 </option>
                                <option VALUE="pxa1928">pxa1928 </option>
                                <option VALUE="pxa1288">pxa1288 </option>
                                <option VALUE="pxa1U88">pxa1U88 </option>
                            </select>
                        </li>
                        <li>
                            <select NAME="Device">
                                <option VALUE="Eden Z2">Eden Z2</option>
                            </select>
                        </li>
                        <li>
                            <select NAME="Branch">
                                <option VALUE="jb42">jb42 </option>
                            </select>
                        </li>
                        <li>
                            <div style="background-color:#FFFFFF">
                                <div class="controls">
                                    <div class="input-append" onload="showLeftTime()" >
                                        <input name="ImageDate" value="2013-11-22" class="datepicker" style="width: 80px;" type="text"/>
                                        <span class="add-on"><i class="icon-calendar"></i></span>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li>
                            <%= submit_tag "Add to Compare", name: "submit", class: "btn btn-info btn-wuxia" %>
                        </li>
                        </fieldset>
                    </ul>
                <% end %>
            </div>
        <!--Grid row-->
            <div  class="row">
                <h2><span>Select Ondemand</span></h2>
            </div>
                <%= render :template => "/ondemand/_task_info" %>
    </div>
<!--top-->
<!--mian-->
</div>
<div class="container" role="main" id="cart">
    <%= render @cart %>
</div>
<!--mian-->
<script>
$(document).ready(function() {
    $('.datepicker').datepicker();
});
</script>