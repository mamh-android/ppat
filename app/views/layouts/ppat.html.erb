<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Welcome to APSE Home Page</title>
    <%= csrf_meta_tag %>
    <meta name="description" content="" />
    <meta name="author" content="Walking Pixels | www.walkingpixels.com" />
    <meta name="robots" content="index, follow" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- CSS styles -->
    <%= stylesheet_link_tag    "blue" %>
    <%= stylesheet_link_tag    "ppat"%>
    <%= stylesheet_link_tag    "daily"%>
    <%= stylesheet_link_tag    "home"%>
    <%= stylesheet_link_tag    "mbExtruder"%>
    <!-- jQuery FullCalendar Styles -->
    <%= stylesheet_link_tag    "plugins/jquery.fullcalendar" %>
    <%= stylesheet_link_tag    "plugins/jquery.plupload.queue"%>
    <%= stylesheet_link_tag    "plugins/jquery.ui.plupload"%>

    <!-- JS Libs -->
    <%= javascript_include_tag "application" %>

    <script>
        $(document).ready(function(){

            // Navbar tooltips
            $('.navbar [title]').tooltip({
                placement: 'bottom'
            });

            // Content tooltips
            $('[role=main] [title]').tooltip({
                placement: 'top'
            });

            // Dropdowns
            $('.dropdown-toggle').dropdown();

        });
    </script>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" /></head>
</head>
<body>


        <!-- Main navigation bar -->
        <header class="navbar navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container">
                    <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".user">
                        <span class="icon-user"></span>
                    </button>
                    <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".navigation">
                        <span class="icon-chevron-down"></span>
                    </button>
                    <!-- <a class="brand" href="http://envato.walkingpixels.com/themes/wuxia/">Walking Pixels Wuxia Bootstrap admin template</a> -->
                    <nav class="nav-collapse navigation">
                        <ul class="nav" role="navigation">
                            <li class="active"><a href="/home/index" title="Home"><span class="icon-home"></span>home</a></li>
                            <li><a href="/tools/gerrit" target="_blank" title="gerrit"><span class="icon-sitemap"></span>gerrit</a></li>
                            <li><a href="/tools/buildbot" title="buildbot"><span class="icon-tasks"></span>buildbot</a></li>
                            <li><a href="/tools/rtvb" title="RTVB"><span class="icon-table"></span>RTVB</a></li>
                             <li><a href="/tools/smoketest" title="smoke test"><span class="icon-font"></span>smoke test</a></li>
                            <!--<li><a href="ui-buttons.html" title="coverity!"><span class="icon-magic"></span>coverity</a></li>-->
                            <li><a href="/tools/oldppat" title="Oldppat!"><span class="icon-plane"></span>Oldppat</a></li>
                            <li><a href="/tools/xref" title="xRef!"><span class="icon-pushpin"></span>xRef</a></li>
                            <li><a href="/daily/index" title="PPAT!"><span class="icon-refresh"></span>PPAT</a></li>
                            <li class="divider-vertical"></li>
                            <li><a href="#" title="Buttons in navbar!" class="btn-wuxia-navbar"><div class="btn btn-wuxia"><span class="icon-key"></span> Widgets</div></a></li>
                        </ul>
                    </nav>
                    <nav class="nav-collapse user" >
                        <div class="user-info pull-right">

                            <div class="btn-group">
                                <% if get_cur_user %>
                                <a class="btn dropdown-toggle" href="#">
                                    <div><strong><%= get_cur_user %></strong></div>
                                    <span class="caret"></span>
                                </a>
                                <% else %>
                                <a class="btn dropdown-toggle" href="/log/in">
                                    <div><strong>Log in</strong></div>
                                    <span class="caret"></span>
                                </a>
                                <% end %>
                                <ul class="dropdown-menu">
                                    <li><a href=""><span class="icon-cogs"></span> Account settings</a></li>
                                    <li><a href=""><span class="icon-bell"></span> Preferences</a></li>
                                    <li><a href=""><span class="icon-envelope"></span> Email notifications</a></li>
                                    <li class="divider"></li>
                                    <li><a href="/log/out"><span class="icon-signout"></span> Logout</a></li>
                                </ul>
                            </div>
                        </div>
                    </nav>
                </div>
            </div>
        </header>
        <!-- /Main navigation bar -->
        <div class="container" role="main">

            <!-- Secondary navigation -->
            <div class="nav-secondary">
                <nav>
                    <ul>
                        <li><a class="wuxify-me" href="/daily/index"><span class="icon-signal"></span>Daily</a></li>
                        <li><a class="wuxify-me" href="/ondemand/index"><span class="icon-picture"></span>Ondemand</a></li>
                        <li><a class="wuxify-me" href="/baremetal/index"><span class="icon-file"></span>Baremetal</a></li>
                        <li><a class="wuxify-me" href="/compare/index"><span class="icon-file"></span>Compare</a></li>
                        <li><a class="wuxify-me" href="/query/index"><span class="icon-calendar"></span>Query</a></li>
                        <li><a class="wuxify-me" href="/trigger/index"><span class="icon-th"></span>Trigger</a></li>
                    </ul>
                </nav>
            </div>
            <!-- /Secondary navigation -->

            <!-- cart -->
            <div id="extruderRight" class="{title:'Compare List', url:'/carts'}"></div>
            <script type="text/javascript">
                $(function(){

                    $("#extruderRight").buildMbExtruder({
                        position:"right",
                        width:300,
                        extruderOpacity:.8,
                        textOrientation:"tb",
                        onExtOpen:function(){},
                        onExtContentLoad:function(){},
                        onExtClose:function(){}
                    });
                });
            </script>
            <div class="content">
            <%= yield %>
            </div>
        </div>
</body>
</html>
