<PlatformConfig>
    <PropertyConfig>
        <!-- platform info -->
        <Property name="platform">EDEN</Property>
        <Property name="soc">pxa1928ff</Property>
        <Property name="board">pxa1928ff</Property>

        <!-- specify board serial number-->
        <Property name="multi_device">true</Property>
        <Property name="device_info">PXA1928_FF_V10_0075</Property>

        <Property name="case_searchpath">case</Property>
        <Property name="run_time"/>

        <!-- backup result for public visit-->
        <Property name="backup_remote">/srv/ftp/ftpshare/ppat/</Property>
        <Property name="log_path">ftp://10.38.32.94/ftpshare/ppat/</Property>

        <Property name="LPM">true</Property>

        <Property name="SyncToDB">true</Property>
    </PropertyConfig>
    <ResourceConfig>
        <Resource name="serialport" port="/dev/ttyUSB2" rate="115200"/>
        <Resource name="adb"/>
        <Resource name="ltk"/>
        <Resource name="relay" port="/dev/ttyS0" rate="9600" user="reset:3;back:4;usb:1" />
    </ResourceConfig>

    <AutoBurnConfig>
        <Property name="last_build_infix">pxa1928</Property>

        <!-- full image path: /autobuild/android/pxa1928/2014-09-03_pxa1928-kk4.4/pxa1928dkb_tz -->
        <!-- image path, need mount to /autobuild-->
        <Property name="image_path_prefix">/autobuild/android/pxa1928</Property>

        <!-- image date infix-->
        <Property name="image_date_infix">pxa1928</Property>

        <!-- image path suffix -->
        <Property name="burn_cmd">sudo ./swdl_linux -D ./EDENFF_DKB/PXA1928_Trusted_eMMC_Hynix.blf -S</Property>
    </AutoBurnConfig>
</PlatformConfig>

