<PlatformConfig>
    <PropertyConfig>
        <!-- platform info -->
        <Property name="platform">HLN3</Property>
        <Property name="soc">pxa1936ff</Property>
        <Property name="board">pxa1936ff</Property>

        <!-- specify board serial number-->
        <Property name="multi_device">true</Property>
        <Property name="device_info">PXA1936_FF_V10_0004, TCMS chip</Property>

        <Property name="case_searchpath">case</Property>
        <Property name="run_time"/>

        <!-- backup result for public visit-->
        <Property name="backup_remote">/srv/ftp/ftpshare/ppat/</Property>
        <Property name="log_path">ftp://10.38.32.98/ftpshare/ppat/</Property>

        <Property name="LPM">true</Property>
    </PropertyConfig>
    <ResourceConfig>
        <Resource name="serialport" port="/dev/ttyUSB0" rate="115200"/>
        <Resource name="adb"/>
        <Resource name="ltk"/>
        <Resource name="relay" port="/dev/ttyS0" rate="9600" user="usb:1" />
    </ResourceConfig>

    <AutoBurnConfig>
        <!-- for generate LAT_BUILD.pxa1928-kk4.4 in autobuild server -->
        <Property name="last_build_infix">pxa1936</Property>

        <!-- full image path: /autobuild/android/pxa1928/2014-09-03_pxa1928-kk4.4/pxa1928dkb_tz -->
        <!-- image path, need mount to /autobuild-->
        <Property name="image_path_prefix">/autobuild/android/pxa1936</Property>

        <!-- image date infix-->
        <Property name="image_date_infix">pxa1936</Property>

        <!-- image path suffix -->
        <Property name="burn_cmd">sudo ./swdl_linux -D ./HLN3FF_DKB/HELAN2_Non-Trusted_Hynix_lpddr3_2g_pop_H9CKNNNBJTMPLR_533Mhz.blf -S</Property>
    </AutoBurnConfig>
</PlatformConfig>

